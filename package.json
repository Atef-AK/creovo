{
  "name": "lensio",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "dev:web": "npm run dev --workspace=@lensio/web",
    "dev:services": "npm run dev --workspace=lensio-api",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "undici": "5.28.4"
  },
  "dependencies": {
    "undici": "5.28.4"
  }
}